{
  "description": "Result of executing a single step.",
  "properties": {
    "step_id": {
      "description": "ID of the executed step",
      "title": "Step Id",
      "type": "string"
    },
    "success": {
      "description": "Whether the step succeeded",
      "title": "Success",
      "type": "boolean"
    },
    "output": {
      "default": null,
      "description": "Output from the step execution",
      "title": "Output"
    },
    "error": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Error message if step failed",
      "title": "Error"
    },
    "duration_ms": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Execution duration in milliseconds",
      "title": "Duration Ms"
    }
  },
  "required": [
    "step_id",
    "success"
  ],
  "title": "StepResult",
  "type": "object"
}