{
  "name": "opsconductor-ai-chat-stress-test",
  "version": "1.0.0",
  "description": "AI Chat Stress Testing Framework for OpsConductor",
  "main": "ai-chat-stress-runner.js",
  "scripts": {
    "stress-test": "node ai-chat-stress-runner.js",
    "stress-test-4h": "node ai-chat-stress-runner.js --duration 4",
    "stress-test-8h": "node ai-chat-stress-runner.js --duration 8",
    "stress-test-fast": "node ai-chat-stress-runner.js --duration 0.5 --interval 1000",
    "test-frontend": "cd frontend && npm test",
    "install-deps": "npm install axios"
  },
  "keywords": ["stress-test", "ai-chat", "load-testing", "opsconductor"],
  "author": "OpsConductor Team", 
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "jest": "^29.7.0"
  }
}