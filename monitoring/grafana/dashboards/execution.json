{
  "dashboard": {
    "title": "OpsConductor - AI Execution",
    "tags": ["opsconductor", "ai", "execution"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(ai_requests_total[5m])) by (status)",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "lastNotNull"]
          }
        }
      },
      {
        "id": 2,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(ai_request_errors_total[5m])) by (reason)",
            "legendFormat": "{{reason}}",
            "refId": "A"
          },
          {
            "expr": "(sum(rate(ai_request_errors_total[5m])) / sum(rate(ai_requests_total[5m]))) * 100",
            "legendFormat": "Error %",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [5],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["B", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "2m",
          "frequency": "1m",
          "name": "AI Execution Error Rate Alert",
          "noDataState": "no_data"
        }
      },
      {
        "id": 3,
        "title": "Request Duration (P95/P99)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum by (le) (rate(ai_request_duration_seconds_bucket[5m])))",
            "legendFormat": "P95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, sum by (le) (rate(ai_request_duration_seconds_bucket[5m])))",
            "legendFormat": "P99",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.50, sum by (le) (rate(ai_request_duration_seconds_bucket[5m])))",
            "legendFormat": "P50",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max", "lastNotNull"]
          }
        }
      },
      {
        "id": 4,
        "title": "Requests by Tool",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(ai_requests_total[5m])) by (tool)",
            "legendFormat": "{{tool}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "lastNotNull"]
          }
        }
      },
      {
        "id": 5,
        "title": "Duration Heatmap",
        "type": "heatmap",
        "gridPos": {"x": 0, "y": 16, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "sum(increase(ai_request_duration_seconds_bucket[1m])) by (le)",
            "format": "heatmap",
            "legendFormat": "{{le}}",
            "refId": "A"
          }
        ],
        "options": {
          "calculate": false,
          "cellGap": 2,
          "color": {
            "exponent": 0.5,
            "fill": "dark-orange",
            "mode": "scheme",
            "scheme": "Spectral",
            "steps": 128
          },
          "exemplars": {
            "color": "rgba(255,0,255,0.7)"
          },
          "filterValues": {
            "le": 1e-9
          },
          "legend": {
            "show": true
          },
          "rowsFrame": {
            "layout": "auto"
          },
          "tooltip": {
            "show": true,
            "yHistogram": false
          },
          "yAxis": {
            "axisPlacement": "left",
            "reverse": false,
            "unit": "s"
          }
        }
      },
      {
        "id": 6,
        "title": "Trace ID Links",
        "type": "table",
        "gridPos": {"x": 0, "y": 24, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "ai_requests_total",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "status": "Status",
                "tool": "Tool"
              }
            }
          }
        ],
        "options": {
          "showHeader": true,
          "footer": {
            "show": false
          }
        },
        "fieldConfig": {
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "trace_id"
              },
              "properties": [
                {
                  "id": "links",
                  "value": [
                    {
                      "title": "View Logs",
                      "url": "/explore?left=%7B%22queries%22:%5B%7B%22expr%22:%22%7Btrace_id%3D%5C%22${__value.text}%5C%22%7D%22%7D%5D%7D"
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "datasource",
          "type": "datasource",
          "query": "prometheus",
          "current": {
            "text": "Prometheus",
            "value": "Prometheus"
          }
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ALERTS{alertstate=\"firing\",component=\"ai_execution\"}",
          "name": "Alerts",
          "step": "60s",
          "tagKeys": "severity",
          "textFormat": "{{alertname}}",
          "titleFormat": "Alert"
        }
      ]
    }
  }
}