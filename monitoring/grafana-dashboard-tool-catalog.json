{
  "dashboard": {
    "title": "Tool Catalog System",
    "tags": ["tool-catalog", "opsconductor", "performance"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "tool_catalog_uptime_seconds",
            "legendFormat": "Uptime (seconds)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 300, "color": "yellow"},
                {"value": 3600, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "rate(tool_catalog_api_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"},
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "pointradius": 5,
        "legend": {
          "show": true,
          "alignAsTable": true,
          "avg": true,
          "current": true,
          "max": true,
          "values": true
        }
      },
      {
        "id": 3,
        "title": "Response Time (P95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(tool_catalog_api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Response Time",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(tool_catalog_api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P99 Response Time",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Response Time"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"},
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [50], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "60s",
          "handler": 1,
          "name": "High Response Time Alert",
          "noDataState": "no_data",
          "notifications": []
        },
        "thresholds": [
          {
            "value": 50,
            "colorMode": "critical",
            "op": "gt",
            "fill": true,
            "line": true
          }
        ]
      },
      {
        "id": 4,
        "title": "Cache Hit Rate",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "(tool_catalog_cache_hits / (tool_catalog_cache_hits + tool_catalog_cache_misses)) * 100",
            "legendFormat": "Cache Hit Rate %",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 60, "color": "yellow"},
                {"value": 80, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Database Query Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 12},
        "targets": [
          {
            "expr": "rate(tool_catalog_db_query_duration_seconds_sum[5m]) / rate(tool_catalog_db_query_duration_seconds_count[5m])",
            "legendFormat": "Avg Query Time",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(tool_catalog_db_query_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Query Time",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Query Time"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"},
        "lines": true,
        "fill": 1,
        "linewidth": 2
      },
      {
        "id": 6,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 12},
        "targets": [
          {
            "expr": "rate(tool_catalog_api_errors_total[5m])",
            "legendFormat": "{{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Errors/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"},
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.01], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "60s",
          "handler": 1,
          "name": "High Error Rate Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 7,
        "title": "Tool Loading Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "rate(tool_catalog_tool_loads_total[5m])",
            "legendFormat": "Tool Loads/sec",
            "refId": "A"
          },
          {
            "expr": "rate(tool_catalog_tool_load_duration_seconds_sum[5m]) / rate(tool_catalog_tool_load_duration_seconds_count[5m])",
            "legendFormat": "Avg Load Time (ms)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Loads/sec"},
          {"format": "ms", "label": "Load Time"}
        ],
        "xaxis": {"mode": "time"},
        "lines": true,
        "fill": 1,
        "linewidth": 2
      },
      {
        "id": 8,
        "title": "Hot Reload Events",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "rate(tool_catalog_hot_reload_total[5m])",
            "legendFormat": "{{trigger_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Reloads/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"},
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "bars": true
      },
      {
        "id": 9,
        "title": "Connection Pool Status",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "tool_catalog_connection_pool_size",
            "legendFormat": "Pool Size",
            "refId": "A"
          },
          {
            "expr": "tool_catalog_connection_pool_available",
            "legendFormat": "Available",
            "refId": "B"
          },
          {
            "expr": "tool_catalog_connection_pool_in_use",
            "legendFormat": "In Use",
            "refId": "C"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto",
          "orientation": "horizontal"
        }
      },
      {
        "id": 10,
        "title": "Cache Statistics",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 8, "y": 28},
        "targets": [
          {
            "expr": "tool_catalog_cache_size",
            "legendFormat": "Cache Size",
            "refId": "A"
          },
          {
            "expr": "tool_catalog_cache_hits",
            "legendFormat": "Cache Hits",
            "refId": "B"
          },
          {
            "expr": "tool_catalog_cache_misses",
            "legendFormat": "Cache Misses",
            "refId": "C"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto",
          "orientation": "horizontal"
        }
      },
      {
        "id": 11,
        "title": "System Health",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 16, "y": 28},
        "targets": [
          {
            "expr": "up{job=\"tool-catalog\"}",
            "legendFormat": "Service Status",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "0", "text": "DOWN"},
              {"type": "value", "value": "1", "text": "UP"}
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      }
    ]
  }
}